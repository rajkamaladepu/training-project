<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Info Properties"
    extraClientlibs="[training.mfminmax,training.mfaltvalidator]"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <info
                jcr:primaryType="nt:unstructured"
                jcr:title="Info"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                        	<title
                                 jcr:primaryType="nt:unstructured"
                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                 fieldLabel="Title"
                                 name="./title"
                                 required="{Boolean}true" />
                             <location
                                 jcr:primaryType="nt:unstructured"
                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                 fieldLabel="location"
                                 name="./location"
                                 required="{Boolean}true" />
                             <phone
                                 jcr:primaryType="nt:unstructured"
                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                 fieldLabel="phone"
                                 name="./phone"
                                 required="{Boolean}true" /> 
                            <email
                                 jcr:primaryType="nt:unstructured"
                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                 fieldLabel="email"
                                 name="./email"
                                 required="{Boolean}true" />
                             <fburl
                                                    jcr:description="Insert Hyperlink"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Facebook URL"
                                                    name="./fburl"
                                                    rootPath="/content/mycity/us/en/business" />
                              <turl
                                                    jcr:description="Insert Hyperlink"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Twitter URL"
                                                    name="./turl"
                                                    rootPath="/content/mycity/us/en/business" />
                               <lurl
                                                    jcr:description="Insert Hyperlink"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Linkedin URL"
                                                    name="./lurl"
                                                    rootPath="/content/mycity/us/en/business" />
                                <iurl
                                                    jcr:description="Insert Hyperlink"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Instagram URL"
                                                    name="./iurl"
                                                    rootPath="/content/mycity/us/en/business" />
                            <infoItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldDescription="Add items"
                                fieldLabel="Info Items"
                                validation="nyc-minmax">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    minimum-items="2"
                                    maximum-items="4" />
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./infoItems">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <title
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Title"
                                                    name="./title" />
                                                <paragraph
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Paragraph Text"
                        							maxlength="{Long}245"
                                                    name="./paragraph" />
                                                <linkpage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Page URl to link"
                                                    name="./linkpage" />
                                                 <pagename
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="pagename"
                                                    name="./pagename" />
                                            </items>
                                        </column>
                                    </items>
                                </field>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    max-items="2" />
                            </infoItems>
                        </items>
                    </column>
                </items>
            </info>
        </items>
    </content>
</jcr:root>